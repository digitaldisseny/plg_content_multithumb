		<field name="prettyphoto_headers" type="list" default="1" label="PRETTYPHOTO_SCRIPTS"
			description="">
			<option value="1">LOAD_AUTOMATICALLY</option>
			<option value="2">LOAD_ALWAYS</option>
			<option value="0">DONT_LOAD</option>
