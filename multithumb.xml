			<option value="alpha">WATERMARK_TRUE_ALPHA_BLENDING</option>
			<option value="color">WATERMARK_TRANSPARENT_BACKGROUND_COLOR</option>
			</field>
		<field name="transparent_color" type="text" default="#000000" label="WATERMARK_TRANSPARENT_COLOR" 
