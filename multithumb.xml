	<config>
	<fields name="params" >  
		<fieldset name="basic">
		<!--<field type="spacer" default="&lt;b&gt;Thumbnail parameters&lt;b&gt;" label="" description="Options for changing the style and behaviour of thumbnail." />-->
		<field name="popup_type" type="list" default="modal" label="POPUP_METHOD"
			description="POPUP_METHOD_DESCR">
			<option value="none">NO_POPUP_JUST_THUMBNAILS</option>
			<option value="modal">JOOMLA_MODAL</option>
			<option value="normal">NORMAL_POPUP</option>
			<option value="thumbnail">DYNAMIC_DRIVE</option>
			<option value="slimbox">SLIMBOX_LIGHTBOX</option>
			<option value="shadowbox">SHADOWBOX</option>
			<option value="prettyPhoto">PRETTY_PHOTO</option>
			<option value="greybox">GREYBOX</option>
			<option value="expansion">THUMBNAIL_EXPANSION</option>
			<option value="expando">SMOOTHLY_EXPANSION</option>
			<option value="iLoad">ILOAD</option>
			</field>

		<field name="min_img_size" type="text" default="20x20" label="MINIMUM_IMAGE_SIZE" 
			description="MINIMUM_IMAGE_SIZE_DESCR" />
		<field name="thumb_proportions" type="list" default="bestfit" label="THUMB_PROPORTIONS"
			description="THUMB_PROPORTIONS_DESCR">
			<option value="bestfit">BEST_FIT</option>
			<option value="crop">CROP_TO_FIT</option>
			<option value="only_cut">CUT_TO_FIT</option>
            <option value="fill">FILL_OUT_WITH_BACKGROUND_COLOR</option>
            <option value="transparent">FILL_OUT_WITH_TRANSPARENT_COLOR</option>
            <option value="stretch">STRECHT_TO_FIT</option>
			</field>
		<field name="thumb_bg" type="text" default="#FFFFFF" label="BACKGROUND_COLOR" 
			description="BACKGROUND_COLOR_DESCR" />
		<!-- <field name="only_not_resized" type="list" default="0" label="Only not resized"
			description="Resize only not resized images">
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field> -->
		
		
		<field name="force_popup" type="list" default="0" label="POPUP_FOR_NON_RESIZED_IMAGES"
			description="POPUP_FOR_NON_RESIZED_IMAGES_DESCR">
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
	</fieldset>
	<fieldset name="ARTICLE_THUMB">
		<field name="enable_thumbs" type="list" default="1" label="THUMBNAILS_FOR_ARTICLES"
			description="THUMBNAILS_FOR_ARTICLES_DESCR">
			<option value="1">ENABLE_FOR_ALL_CATEGORIES</option>
			<option value="2">ENABLE_FOR_FOLLOWING_CATEGORIES_ONLY</option>
			<option value="3">ENABLE_FOR_ALL_CATEGORIES_EXCEPT_FOLLOWING</option>
			<option value="4">ENABLE_FOR_FOLLOWING_CLASSES_ONLY</option>
			<option value="0">DISABLE</option>
		</field>
		<field name="only_cats" multiple="multiple"  type="category" extension="com_content" size="5" default=""
			label="CATEGORIES_OF_ARTICLES"
			description="CATEGORIES_OF_ARTICLES_DESCR" /> 
		<field name="only_classes" type="text" default="" label="IMAGE_CLASSES" 
			description="IMAGE_CLASSES_DESCR" />

		<field name="use_image_size" type="list" default="0" label="USE_ARTICLE_IMAGE_SIZE"
			description="USE_ARTICLE_IMAGE_SIZE_DESCR">
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
		<field name="thumb_size" type="text" default="150x100" label="THUMBS_SIZE"
			description="THUMBS_SIZE_DESCR">
		</field>
		<field name="thumb_size_first" type="text" default="" label="LEADING_THUMB_SIZE"
			description="LEADING_THUMB_SIZE_DESCR">
		</field>
		<field name="leading_num" type="text" default="1" label="LEADING_IMAGES_NUMBER"
			description="LEADING_IMAGES_NUMBER_DESCR">
		</field>
		<field name="thumbclass" type="text" default="multithumb" label="THUMBNAILS_CLASS" 
			description="THUMBNAILS_CLASS_DESCR" />
		<field name="css" type="textarea" cols="30" rows="3" default="
.multithumb {&lt;br /&gt; 
   margin: 5px; &lt;br /&gt;
   float: left; &lt;br /&gt;
}" label="THUMBNAIL_CSS"
	  description="THUMBNAIL_CSS_DESCR" />
	</fieldset>  
	<fieldset name="BLOG_THUMB_PARAMETERS" addfieldpath="/plugins/content/multithumb/fields" >
		<field name="enable_blogs" type="list" default="1" label="THUMBNAILS_FOR_BLOGS"
			description="THUMBNAILS_FOR_BLOGS_DESCR">
			<option value="1">ENABLE_FOR_ALL_BLOGS</option>
			<option value="2">ENABLE_FOR_FOLLOWING_BLOGS_ONLY</option> 
			<option value="4">ENABLE_FOR_FOLLOWING_CLASSES_ONLY</option>
			<option value="0">DISABLE</option>
		</field>
		<field name="blog_ids" multiple="multiple"  type="blogs" default="" label="THUMBNAILS_FOR_BLOGS" 
			description="THUMBNAILS_FOR_BLOGS_DESCR" />			
		<field name="only_classes_blog" type="text" default="" label="IMAGE_CLASSES" 
			description="IMAGE_CLASSES_DESCR" />
		<field name="blog_mode" type="list" default="link" label="BLOG_MODE" 
			description="BLOG_MODE_DESCR" >
			<option value="link">LINK_TO_ARTICLE</option>
			<option value="popup">POPUP</option>
			<option value="thumb">THUMBNAILS_ONLY</option>
			<!-- <option value="disable">Disable</option> -->
		</field>
		<field name="blog_img_txt" type="list" default="0" label="IMAGE_TOOLTIP" 
			description="IMAGE_TOOLTIP_DESCR">
			<option value="0">ARTICLE_READMORE</option>
			<option value="1">IMG_ALT</option>
			<option value="2">IMG_TITLE</option>
		</field>
		<field name="use_image_size_blog" type="list" default="0" label="USE_ARTICLE_IMAGE_SIZE"
			description="USE_ARTICLE_IMAGE_SIZE_DESCR">
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
		<field name="blog_size" type="text" default="200x150" label="BLOGS_THUMBS_SIZE"
			description="BLOGS_THUMBS_SIZE_DESCR">
		</field>			
		<field name="blog_size_leading" type="text" default="" label="LEADING_ARTICLE_THUMB_SIZE"
			description="LEADING_ARTICLE_THUMB_SIZE_DESCR">
		</field>
		<field name="blog_leading_num" type="text" default="1" label="LEADING_ARTICLE_NUMBER"
			description="LEADING_ARTICLE_NUMBER_DESCR">
		</field>
		<field name="thumbclass_blog" type="text" default="multithumb_blog" label="THUMBNAILS_CLASS" 
			description="THUMBNAILS_CLASS_DESCR" />
		<field name="css_blog" type="textarea" cols="30" rows="3" default=".multithumb_blog {    margin: 5px;    float: left; }" 
		label="THUMBNAIL_CSS"
	  description="THUMBNAIL_CSS_DESCR" />
	</fieldset>
	<fieldset name="BORDER_PARAMETERS">
		<field name="border_style" type="list" default="none" label="BORDER_STYLE" 
			description="BORDER_STYLE_DESCR">
			<option value="none">PRESERVE</option>
			<option value="remove">REMOVE</option>
			<option value="solid">SOLID</option>
			<option value="dotted">DOTTED</option>
			<option value="dashed">DASHED</option>
			<option value="groove">GROOVE</option>
			<option value="ridge">RIDGE</option>
			<option value="inset">INSET</option>
			<option value="outset">OUTSET</option>
			<option value="double">DOUBLE</option>
		</field>
		<field name="border_size" type="text" default="2px" label="BORDER_SIZE" 
			description="BORDER_SIZE_DESCR" >
			<!-- <option value="0">0px</option> 
			<option value="1px">1px</option>
			<option value="2px">2px</option>
			<option value="3px">3px</option>
			<option value="4px">4px</option>
			<option value="5px">5px</option>-->
		</field>
		<field name="border_color" type="text" default="#000000" label="BORDER_COLOR" 
			description="BORDER_COLOR_DESCR"/>
	</fieldset>
	<fieldset name="CAPTION_PARAMETERS">
		<field name="caption" type="list" default="0" label="CAPTION"
			description="">
			<option value="0">DISABLE</option>
			<option value="1">FOR_ARTICLES_ONLY</option>
			<option value="2">FOR_ARTICLES_AND_BLOGS</option>
			<option value="3">FOR_BLOGS_ONLY</option>
		</field>
		<field name="caption_pos" type="list" default="bottom" label="POSITION"
			description="POSITION_DESCR">
			<!-- <option value="disabled">Disabled</option> -->
			<option value="bottom">BOTTOM</option>
			<option value="top">TOP</option>
			<option value="right">RIGTH</option>
			<option value="left">LEFT</option>
			</field>
		<field name="caption_type" type="list" default="title" label="CAPTION_FROM"
			description="CAPTION_FROM_DESCR">
			<option value="alt">ALT_TEXT</option>
			<option value="title">TITLE</option>
			<option value="iptc_caption">IPTC_CAPTION</option>
			<option value="alt_or_title">ALT_TEXT_OR_TITLE</option>
			<option value="title_or_alt">TITLE_OR_ALT_TEXT</option>
			<option value="iptc_caption_or_alt_or_title">IPTC_CAPTION_OR_ALT_TEXT_OR_TITLE</option>
			<option value="iptc_caption_or_title_or_alt">IPTC_CAPTION_OR_TITLE_OR_ALT_TEXT</option>
			<option value="alt_or_title_or_iptc_caption">ALT_TEXT_OR_TITLE_OR_IPTC_CAPTION</option>
			<option value="title_or_alt_or_iptc_caption">TITLE_OR_ALT_TEXT_IPTC_CAPTION</option>
		</field>
		<field name="caption_class" type="text" default="mtCapStyle" label="CAPTION_CLASS" 
			description="CAPTION_CLASS_DES" />
		<field name="caption_css" type="textarea" cols="30" rows="7" default="
.mtCapStyle {&lt;br /&gt; 
   font-weight: bold;&lt;br /&gt;
   color: black;&lt;br /&gt;
   background-color: #ddd;&lt;br /&gt;
   margin: 0px 4px;&lt;br /&gt;
   text-align:center;&lt;br /&gt;
   white-space: pre-wrap;&lt;br /&gt;
}" label="CAPTION_CSS_STYLING"
	  description="CAPTION_CSS_STYLING_DESCR" />
		
	</fieldset>
	<fieldset name="GROUPING_PARAMETERS">
		<field name="group_images" type="list" default="0" label="GROUP_IMAGES"
			description="GROUP_IMAGES_DESCR">
			<option value="0">JNO</option>
			<option value="1">OF_ARTICLE</option>
			<option value="2">ON_PAGE</option>
		</field>	  
		<field name="group_images_gal" type="list" default="1" label="GALLERY_GROUPING"
			description="GALLERY_GROUPING_DESCR">
			<option value="0">AS_ABOVE</option>
			<option value="1">SEPARATELY</option>
		</field>	  

		
	  <field name="max_thumbnails" type="text" default="1" label="GROUP_PREVIEW"
			description="GROUP_PREVIEW_DESCR" />
		  <field name="more_images" type="list" default="1" label="MORE_IMAGES"
			description="MORE_IMAGES_DESCR">
			<option value="1">SHOW</option>
			<option value="0">HIDE</option>
	</field>
		  <field name="more_images_text" type="text" default="MORE_IMAGES_DEFAULT" label="MORE_IMAGES_TEXT"
			description="MORE_IMAGES_TEXT_DESCR" />
	</fieldset>
	<fieldset name="GALLERY_PARAMETERS">
		<field name="num_cols" type="text" default="3" label="COLUMNS_NUM" 
			description="COLUMNS_NUM_DESCR" />
		<field name="gallery_class" type="text" default="mtGallery" label="GIVE_GALLERY_THIS_CLASS" 
			description="GIVE_GALLERY_THIS_CLASS_DESCR" />
		<field name="gallery_css" type="textarea" cols="30" rows="4" default="
.mtGallery {&lt;br /&gt; 
   margin: 5px; &lt;br /&gt;
   align: center; &lt;br /&gt;
   float: none; &lt;br /&gt;
}" label="GALLERY_CSS"
	  description="GALLERY_CSS_DESCR" />
    <field type="spacer" label="CAPTION_FROM"        description="CAPTION_FROM_DESCR"        />
    <field type="spacer" label="ORDER_OF_PREFERENCE" description="ORDER_OF_PREFERENCE_DESCR" /> 
		<field name="caption_type_gallery_iptc"     type="text" size="1" default="1" label="IMAGE_IPTC_CAPTION"	description="IMAGE_IPTC_CAPTION_DESCR" />
		<field name="caption_type_gallery_filename" type="text" size="1" default="0" label="IMAGE_FILENAME"	    description="IMAGE_FILENAME_DESCR" />
		<field name="caption_type_gallery_title"    type="text" size="1" default="2" label="IMAGE_TITLE"	description="IMAGE_TITLE_DESCR" />
		<field name="caption_type_gallery_alt"      type="text" size="1" default="3" label="MAIN_IMAGE_ALT"	    description="MAIN_IMAGE_ALT_DESCR" />
	</fieldset>
	<fieldset name="FULL_SIZE_IMAGE_PARAMETERS">
		<field name="resize" type="list" default="0" label="RESIZE_IMAGES"
			description="RESIZE_IMAGES_DESCR">
			<option value="0">Disable</option>
			<option value="1">Enable</option>
			</field>
		<field name="full_width" type="text" default="800" label="WIDTH" 
				description="WIDTH_DESCR" />
		<field name="full_height" type="text" default="600" label="HEIGHT" 
				description="HEIGHT_DESCR" />
		<field name="image_proportions" type="list" default="bestfit" label="IMAGE_PROPORTIONS"
			description="IMAGE_PROPORTIONS_DESCR">
			<option value="bestfit">BEST_FIT</option>
			<option value="crop">CROP_TO_FIT</option>
			<!-- <option value="cut">Cut to fit (cut both sides)</option> -->
            <option value="fill">FILL_OUT_WITH_BACKGROUND_COLOR</option>
            <option value="transparent">FILL_OUT_WITH_TRANSPARENT_COLOR</option>
            <option value="stretch">STRECHT_TO_FIT</option>
			</field>
		<field name="image_bg" type="text" default="#000000" label="BACKGROUND_COLOR" 
				description="BACKGROUND_COLOR_DESCR" />
	</fieldset>
	<fieldset name="MAGNIFYING">
		<field name="magnify_type" type="list" default="1" label="MAGNIFYING_SYMBOLS"
			description="MAGNIFYING_SYMBOLS_DESCR">
			<option value="0">MAGNIFYING_NOTHING</option>
			<option value="1">MAGNIFYING_MAGNIFY_CURSOR</option>
			<option value="2">MAGNIFYING_ZOOMIN_LABEL</option>
			<option value="3">MAGNIFYING_MAGNIFY_CURSOR_AND_ZOOMIN_LABEL</option>
		</field>
		<field name="zoomin_file" type="text" default="plugins/content/multithumb/zoomin.png" label="MAGNIFYING_ZOOM_LABEL_FILE" 
			description="MAGNIFYING_ZOOM_LABEL_FILE_DESCR" />
		<field name="zoomin_position" type="list" default="0" label="MAGNIFYING_ZOOM_LABEL_POSITION"
			description="">
			<option value="0">MAGNIFYING_CENTER</option>
			<option value="1">MAGNIFYING_LEFT_TOP</option>
			<option value="2">MAGNIFYING_RIGHT_TOP</option>
			<option value="3">MAGNIFYING_RIGHT_BOTTOM</option>
			<option value="4">MAGNIFYING_LEFT_BOTTOM</option>
		</field>			
	</fieldset>
	<fieldset name="WATERMARK">
		<field name="watermark_type" type="list" default="0" label="WATERMARK"
			description="">
			<option value="0">WATERMARK_NOTHING</option>
			<option value="1">WATERMARK_FULL_SIZE_IMAGES_ONLY</option>
			<option value="2">WATERMARK_FULL_SIZE_IMAGES_AND_THUMBNAIL</option>
		</field>
		<field name="watermark" type="list" default="0" label="WATERMARK_ENABLE_FOR"
			description="WATERMARK_ENABLE_FOR_DESCR">
			<!-- <option value="0">Disable</option> -->
			<option value="1">WATERMARK_ALL_CATEGORIES</option>
			<option value="2">WATERMARK_FOLLOWING_CATEGORIES_ONLY</option>
			<option value="3">WATERMARK_ALL_CATEGORIES_EXCEPT_FOLLOWING</option>
		</field>
		<field name="watermark_cats" multiple="multiple"  type="category" extension="com_content" default="" label="WATERMARK_CATEGORIES_OF_ARTICLES" 
			description="WATERMARK_CATEGORIES_OF_ARTICLES_DESCR" />
		<field name="watermark_file" type="text" default="" label="WATERMARK_FILE" 
