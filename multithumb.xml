		<field name="slimbox_counterText" type="text" default="Image {x} of {y}" label="COUNTERTEXT" description="COUNTERTEXT_DESCR"/>
		<field name="slimbox_closeKeys" type="text" default="[27, 88, 67]" label="CLOSE_KEYS" description="CLOSE_KEYS_DESCR"/>
		<field name="slimbox_previousKeys" type="text" default="[37, 80]" label="PREVIOUS_KEYS" description="PREVIOUS_KEYS_DESCR"/>
		<field name="slimbox_nextKeys" type="text" default="[39, 78]" label="NEXT_KEYS" description="NEXT_KEYS_DESCR"/>
