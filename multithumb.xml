			<option value="off">JNO</option>
			<option value="md5">WATERMARK_MD5</option>
			<option value="crc32">WATERMARK_CRC32</option>
			</field>
	</fieldset>
	<fieldset name="SLIMBOX_PARAMETERS">
		<field name="slimbox_headers" type="list" default="1" label="SLIMBOX_SCRIPTS" description="">
			<option value="1">LOAD_AUTOMATICALLY</option>
			<option value="2">LOAD_ALWAYS</option>
			<option value="0">DONT_LOAD</option>
		</field>
		<field name="slimbox_loop" type="list" default="0" label="LOOP" description="LOOP_DESCR">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
		<field name="slimbox_overlayOpacity" type="text" default="0.8" label="OVERLAY_OPACITY" description="OVERLAY_OPACITY_DESCR"/>
		<field name="slimbox_overlayFadeDuration" type="text" default="400" label="OVERLAY_FADE_DURATION" description="OVERLAY_FADE_DURATION_DESCR"/>
		<field name="slimbox_resizeDuration" type="text" default="400" label="RESIZE_DURATION" description="RESIZE_DURATION_DESCR"/>
		<field name="slimbox_initialWidth" type="text" default="250" label="INITIAL_WIDTH" description="INITIAL_WIDTH_DESCR"/>
		<field name="slimbox_initialHeight" type="text" default="250" label="INITIAL_HEIGHT" description="INITIAL_HEIGHT_DESCR"/>
