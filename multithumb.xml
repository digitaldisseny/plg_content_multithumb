			<option value="center">WATERMARK_CENTER</option>
			<option value="left">WATERMARK_SHIFT_FROM_LEFT</option>
			<option value="right">WATERMARK_SHIFT_FROM_RIGHT</option>
		</field>
		<field name="watermark_left" type="text" default="" label="WATERMARK_HORIZ_SHIFT" 
			description="WATERMARK_HORIZ_SHIFT_DESCR" />
		<field name="watermark_vert_type" type="list" default="" label="WATERMARK_VERT_POSITION" 
			description="" >
			<option value="middle">WATERMARK_MIDDLE</option>
			<option value="top">WATERMARK_SHIFT_FROM_TOP</option>
			<option value="bottom">WATERMARK_SHIFT_FROM_BOTTOM</option>
		</field>
		<field name="watermark_top" type="text" default="" label="WATERMARK_VERTICAL_INDENT" 
			description="WATERMARK_VERTICAL_INDENT_DESR" />
		<field name="transparency_type" type="list" default="alpha" label="WATERMARK_TRANSPARENCY"
